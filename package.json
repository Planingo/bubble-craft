  "scripts": {
    "before-push": "yarn test:generate-output",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=${CHROMATIC_PROJECT_TOKEN} --exit-zero-on-changes",
    "lint": "eslint",
    "format:base": "yarn prettier src Template .storybook cypress public scripts",
    "format": "yarn format:base --write",
    "format:check": "yarn format:base --check",
    "dev": "yarn storybook",
    "storybook": "storybook dev -p 9009",
    "test": "vitest",
    "test:e2e:run": "yarn cypress run --component --browser chrome",
    "test:e2e:parallel": "cypress-parallel -s test:e2e:run -t 3 -d src/e2e -a '\"--config baseUrl=http://localhost:3000\"' --reporter cypress-multi-reporters --reporter-options configFile=multi-reporter.config.json",
    "test:e2e:open": "yarn cypress open",
    "test:e2e:merge-report": "npx mochawesome-merge 'tests-results/*-report.json' > tests-results/report.json && rm -f tests-results/*-report.json",
    "test:e2e:to-html": "npx marge tests-results/report.json && rm -f tests-results/report.json",
    "test:e2e": "yarn test:e2e:parallel && yarn test:e2e:merge-report && yarn test:e2e:to-html"
  },
