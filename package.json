{
  "name": "@pixel-brew/bubble-craft",
  "version": "2.3.0",
  "license": "MIT",
  "description": "Design system de Pixel Brew",
  "main": "src/index.js",
  "module": "src",
  "author": {
    "name": "Bugsyaya"
  },
  "scripts": {
    "before-push": "yarn test:generate-output",
    "build": "react-scripts build",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=${CHROMATIC_PROJECT_TOKEN} --exit-zero-on-changes",
    "dev": "yarn storybook",
    "start": "react-scripts start",
    "storybook": "storybook dev -p 9009",
    "storybook:start": "yarn storybook:copy-public && yarn start-storybook -p 6006 -s ./public",
    "test": "react-scripts test",
    "test:e2e:run": "yarn cypress run --component --browser chrome",
    "test:e2e": "cypress-parallel -s test:e2e:run -t 3 -d src/e2e -a '\"--config baseUrl=http://localhost:3000\"' --reporter cypress-multi-reporters --reporter-options configFile=multi-reporter.config.json",
    "test:e2e:open": "yarn cypress open",
    "merge:report": "npx mochawesome-merge 'tests-results/*-report.json' > report.json && rm -f tests-results/*.json",
    "to:html": "npx marge report.json && rm -f report.json",
    "test:e2e:with:report": "yarn test:e2e && yarn merge:report && yarn to:html"
  },
  "peerDependencies": {
    "@fullcalendar/resource": "*",
